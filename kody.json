    "google-chrome",
    "google-drive",
    "quickcast",
    "the-unarchiver",
    "google-hangouts",
    "heroku-toolbelt",
    "spotify",
    "atom",
    "alfred",
    "flux",
    "skype",
    "sublime-text"
  ],
  "global_npm_modules": {
    "sike": "latest",
    "less": "latest",
    "jade": "latest",
    "gulp": "latest",
    "grunt-cli": "latest",
